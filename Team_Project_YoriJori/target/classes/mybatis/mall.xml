<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mall.model.MallBean">

<select id="GetAllProduct" resultType="mall.model.ProductBean">
select * from product

<if test="whatColumn=='se'">
where pd_category like #{keyword}
</if>
</select>

</mapper>